#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/apple/a/bumpp-demo/node_modules/.pnpm/bumpp@10.3.2/node_modules/bumpp/bin/node_modules:/Users/apple/a/bumpp-demo/node_modules/.pnpm/bumpp@10.3.2/node_modules/bumpp/node_modules:/Users/apple/a/bumpp-demo/node_modules/.pnpm/bumpp@10.3.2/node_modules:/Users/apple/a/bumpp-demo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/apple/a/bumpp-demo/node_modules/.pnpm/bumpp@10.3.2/node_modules/bumpp/bin/node_modules:/Users/apple/a/bumpp-demo/node_modules/.pnpm/bumpp@10.3.2/node_modules/bumpp/node_modules:/Users/apple/a/bumpp-demo/node_modules/.pnpm/bumpp@10.3.2/node_modules:/Users/apple/a/bumpp-demo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/bumpp.mjs" "$@"
else
  exec node  "$basedir/../../bin/bumpp.mjs" "$@"
fi
